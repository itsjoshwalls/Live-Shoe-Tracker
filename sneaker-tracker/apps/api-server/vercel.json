{
  "version": 2,
  "installCommand": "cd ../.. && pnpm install",
  "buildCommand": "pnpm run build",
  "functions": {
    "api/index.ts": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.ts" }
  ],
  "regions": ["iad1"],
  "env": {
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase_service_role_key",
    "SUPABASE_URL": "@supabase_url"
  }
}
