    "firebase-functions": "^5.0.0",
    "@google-cloud/bigquery": "^7.9.0",
    "@google-cloud/pubsub": "^4.8.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "firebase-functions-test": "^3.1.0",
    "firebase": "^11.0.0"
