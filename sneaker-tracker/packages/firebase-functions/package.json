{
  "name": "@sneaker-tracker/firebase-functions",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "index.js",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "echo Skipping build for pure ESM JS functions",
